import java.util.Scanner;
class mtable{
	public static void main(String args[]){
		Scanner ss=new Scanner(System.in);
		System.out.println("enter a number: ");
		int num=ss.nextInt();
		System.out.println("Table of "+num);	
		for(int i=1;i<=10;i++){
			System.out.println(num+"x"+i+"="+(num*i));
		}
		ss.close();
	}
}
