class FSS {
    public static void main(String args[]) {
        int n=10;
        System.out.println("Fibonacci Series:");
        for (int i = 0; i < n; i++) {
            System.out.println(Fibonacci(i));
        }
    }

    public static int Fibonacci(int a) {
        if (a <= 1) {
            return a;
        } else {
            return Fibonacci(a - 1) + Fibonacci(a - 2);
        }
    }
}
